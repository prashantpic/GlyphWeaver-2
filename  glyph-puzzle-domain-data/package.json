{
  "name": "glyph-puzzle-domain-data",
  "version": "1.0.0",
  "description": "Data access layer and domain modeling using Mongoose for MongoDB, and Redis for caching for the Glyph Puzzle game.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "migrate": "migrate-mongo up",
    "migrate:down": "migrate-mongo down",
    "migrate:create": "migrate-mongo create",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://your-repository-url/glyph-puzzle-domain-data.git"
  },
  "keywords": [
    "mongoose",
    "mongodb",
    "redis",
    "ioredis",
    "typescript",
    "data-access",
    "domain-driven-design"
  ],
  "author": "Glyph Puzzle Team",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.5",
    "ioredis": "^5.4.1",
    "lodash": "^4.17.21",
    "mongoose": "^8.3.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/ioredis": "^5.0.0",
    "@types/lodash": "^4.17.4",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "express": "^4.18.2",
    "migrate-mongo": "^11.0.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}