{
  "name": "glyph-puzzle-service-orchestrator",
  "version": "1.0.0",
  "description": "Core business process orchestrator managing complex workflows across multiple services for Glyph Puzzle game.",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "temporal",
    "orchestration",
    "workflow",
    "saga",
    "nodejs",
    "typescript"
  ],
  "author": "GlyphPuzzleTeam",
  "license": "ISC",
  "dependencies": {
    "@temporalio/activity": "^1.9.3",
    "@temporalio/client": "^1.9.3",
    "@temporalio/worker": "^1.9.3",
    "@temporalio/workflow": "^1.9.3",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "joi": "^17.13.3",
    "winston": "^3.13.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "~5.3.3"
  },
  "engines": {
    "node": "20.14.0"
  }
}