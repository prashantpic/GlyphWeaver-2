{
  "name": "glyph-puzzle-security",
  "version": "1.0.0",
  "description": "Backend service for central security concerns including authentication, authorization, IAP receipt validation, cryptography, and secure secret management for the Glyph Puzzle application.",
  "main": "dist/src/index.js",
  "scripts": {
    "start": "node dist/src/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "jest",
    "prepare": "npm run build"
  },
  "engines": {
    "node": ">=20.14.0"
  },
  "author": "Glyph Puzzle Team",
  "license": "ISC",
  "dependencies": {
    "express": "^4.19.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-apple": "^1.1.1",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "aws-sdk": "^2.1628.0",
    "axios": "^1.7.2",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.2.0",
    "class-validator": "^0.14.1",
    "class-transformer": "^0.5.1",
    "dotenv": "^16.4.5",
    "reflect-metadata": "^0.2.2",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/express": "^4.17.21",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-apple": "^1.1.3",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.0",
    "@types/supertest": "^6.0.2",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.4",
    "nodemon": "^3.1.2",
    "ts-node": "^10.9.2",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.0",
    "supertest": "^7.0.0"
